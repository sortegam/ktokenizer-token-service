{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "max-len": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ],
    "object-curly-newline": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
